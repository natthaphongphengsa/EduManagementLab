@inject EduManagementLab.Core.Services.UserService userService;
@{
    var userId = User.Claims.FirstOrDefault(c => c.Type == "sub").Value;
}
<ul class="navbar-nav">
    <li class="nav-item">
        <a class="nav-link" asp-page="/Account/Index">@userService.GetUser(Guid.Parse(userId)).UserName</a>
    </li>
    <li class="nav-item">
        <a class="nav-link text-dark" asp-controller="Home" asp-action="Logout">Logout</a>
    </li>
</ul>
